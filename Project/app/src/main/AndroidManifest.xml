        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-7220530792805172~3627817921"
            />

        <activity android:name="learnhubstudio.auditing.mcqs" />
        <activity android:name="learnhubstudio.auditing.splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="learnhubstudio.auditing.MainActivity" />
        <activity android:name="learnhubstudio.auditing.chapter1" />
        <activity android:name="learnhubstudio.auditing.chapter2" />
        <activity android:name="learnhubstudio.auditing.chapter3" />
        <activity android:name="learnhubstudio.auditing.chapter4" />
        <activity android:name="learnhubstudio.auditing.chapter5" />
        <activity android:name="learnhubstudio.auditing.chapter6" />
        <activity android:name="learnhubstudio.auditing.chapter7" />
        <activity android:name="learnhubstudio.auditing.chapter8" />
        <activity android:name="learnhubstudio.auditing.chapter9" />
        <activity android:name="learnhubstudio.auditing.chapter10" />
        <activity android:name="learnhubstudio.auditing.chapter11" />
        <activity android:name="learnhubstudio.auditing.chapter12" />
        <activity android:name="learnhubstudio.auditing.chapter13" />
        <activity android:name="learnhubstudio.auditing.chapter14" />
        <activity android:name="learnhubstudio.auditing.about" />
        <activity android:name="learnhubstudio.auditing.menu" />
        <activity android:name="learnhubstudio.auditing.privacy_policy" />


        <service android:name="learnhubstudio.auditing.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>